{
  "name": "flux-angular",
  "version": "4.0.0-alpha",
  "description": "A FLUX architecture for Angular JS",
  "main": "dist/flux-angular.cjs.js",
  "module": "dist/flux-angular.esm.js",
  "browser": "dist/flux-angular.umd.js",
  "scripts": {
    "bootstrap": "npm ci",
    "build": "rollup -c",
    "develop": "rollup -c -w",
    "format": "run-s format:eslint format:prettier",
    "format:eslint": "eslint --fix \"**/*.js\"",
    "format:prettier": "prettier --write \"**/*.{js,json,md,yml}\"",
    "lint": "eslint \"**/*.js\"",
    "test": "karma start --single-run",
    "karma": "karma start",
    "pretest": "run-s lint build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,css,md,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/christianalfoni/flux-angular.git"
  },
  "author": "Christian Alfoni",
  "license": "MIT",
  "devDependencies": {
    "angular": "^1.6.4",
    "angular-mocks": "^1.6.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^1.0.0-rc.13",
    "jasmine-core": "^3.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-rollup-preprocessor": "^6.0.1",
    "lint-staged": "^7.2.2",
    "npm-run-all": "^4.1.3",
    "prettier": "1.14.2",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0"
  },
  "peerDependencies": {
    "angular": "^1.6.4"
  },
  "dependencies": {
    "baobab": "^2.4.3",
    "dispatchr": "^0.3.3"
  }
}
